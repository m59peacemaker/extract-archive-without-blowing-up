	"license": "CC0-1.0",
	"dependencies": {
		"exodus-to-node": "git+https://github.com/m59peacemaker/exodus-to-node.git#84870372d16e79c88599261d0500205347d08ef2",
		"node-7z": "^2.1.1",
		"pascalcase": "^1.0.0",
		"promisify-child-process": "^4.1.1",
		"split2": "^3.1.1"
	},
	"devDependencies": {
		"@ramda/sortby": "^0.26.1",
		"fs-extra": "^9.0.1",
		"uuid-v4": "^0.1.0",
		"zora": "^4.0.0"
