{
	"name": "extract-archive-without-blowing-up",
	"version": "0.2.1",
	"description": "Recursively extract archives up to a size limit.",
	"main": "src/index.js",
	"scripts": {
		"test": "node src/index.spec.js",
		"postinstall": "exodus-to-node --outdir './node_modules/.bin' './exodus-bundles/*'"
